      "source": [
        "\n",
        "\n",
        "x=int(input(\"Enter a number\"))\n",
        "temp=x\n",
        "rev=0\n",
        "\n",
        "while(x > 0):\n",
        "    a = x % 10\n",
        "    rev = rev*10 + a\n",
        "    x = x//10\n",
        "\n",
        "if (temp==rev):\n",
        "    print(\"Palindrome \")\n",
        "else:\n",
        "    print(\"Not Palindrome\")\n",
        ""
      ]
